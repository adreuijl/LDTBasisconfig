GRAPH config:Theatre {
  
  config:SearchAPIData a elmo:InformationProduct;
    elmo:backend config:LocalBackend;
    elmo:resultType elmo-rt:Tuple;
    elmo:optionalParameter config:ApiParameter;
    elmo:query """
      PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
      PREFIX skos: <http://www.w3.org/2004/02/skos/core#>
      PREFIX dct: <http://purl.org/dc/terms/>
      PREFIX aquo: <http://data.aquo.nl/def/aquo#>
       
      SELECT distinct ?search_label ?type 
      WHERE {
        {
          GRAPH <http://data.aquo.nl/id/dataset/begrippen> {
            ?begrip a  skos:Concept;
                       skos:prefLabel ?begrip_label.
            OPTIONAL {?begrip skos:altLabel ?synoniem}                 
            OPTIONAL {?begrip skos:definition ?definitie}
          }
          OPTIONAL {
            GRAPH <http://data.aquo.nl/id/dataset/begrippen> {
              ?begrip dct:subject ?categorie_begrip.
            }
            GRAPH <http://data.aquo.nl/id/dataset/categorieen> {
              ?categorie_begrip rdfs:label ?categorie_begrip_label
            }
          }
          BIND(IF(BOUND(?begrip_label),?begrip_label,"") as ?search_label)
          BIND(IF(BOUND(?begrip_label),"Begrip","") as ?type)
          FILTER regex(?search_label,concat("^",?api),"i")
        }
        UNION {
            GRAPH <http://data.aquo.nl/id/dataset/categorieen> {
              ?categorie rdfs:label ?categorie_label
            }
          
          BIND(IF(BOUND(?categorie_label),?categorie_label,"") as ?search_label)
          BIND(IF(BOUND(?categorie_label),"Categorie","") as ?type)
          FILTER regex(?search_label,concat("^",?api),"i")
        }
        UNION {
        
          GRAPH <http://data.aquo.nl/id/dataset/domeintabel> {
           <http://data.aquo.nl/id/dataset/domeintabel> dct:hasPart ?domeintabel.
           ?domeintabel rdfs:label ?domeintabel_label
          }

          BIND(IF(BOUND(?domeintabel_label),?domeintabel_label,"") as ?search_label)
          BIND(IF(BOUND(?domeintabel_label),"Domeintabel","") as ?type)
          FILTER regex(?search_label,concat("^", ?api),"i")
        }
        
        UNION {
          
          
          GRAPH <http://data.aquo.nl/id/dataset/domeintabel> {
              ?domeintabel rdfs:label ?domeintabel_label
          }
          GRAPH ?domeintabel {
               ?id aquo:Omschrijving ?domeintabelwaarde_label.
          }
          
          BIND(IF(BOUND(?domeintabelwaarde_label),?domeintabelwaarde_label,"") as ?search_label)
          BIND(IF(BOUND(?domeintabelwaarde_label),"Domeintabel waarde","") as ?type)
          FILTER regex(?search_label,concat("^", ?api),"i")
            
        }
        
        
       
      }
    """
  .
    
  
  
}